  "version": "0.2.0",
  "configurations": [
    {
      // https://stackoverflow.com/a/61856774/1363247
      "name": "deno (current file)",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "-A",
        "--unstable",
        "--inspect-brk",
        // "--importmap=./import_map.json",
        "${file}"
      ],
      "port": 9229,
      "outputCapture": "std"
      // "stopOnEntry": true
    }
  ]
}
